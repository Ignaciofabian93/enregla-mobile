{
  "expo": {
    "name": "enregla",
    "slug": "enregla-mobile",
    "scheme": "enregla",
    "version": "2.0.12",
    "orientation": "portrait",
    "icon": "./assets/icons/enregla-icon.png",
    "userInterfaceStyle": "light",
    "privacy": "public",
    "platforms": ["android"],
    "splash": {
      "image": "./assets/icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/enregla-icon.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode": 14,
      "package": "com.ignaciofabian93.enreglamobile",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_INTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "plugins": [
      "expo-router",
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesClearTextTraffic": true
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permita a enregla utilizar su ubicaci√≥n."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Permita a enregla acceder a sus fotos."
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "93d67ee6-d038-4e8e-92aa-af2129403975"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "sdkVersion": "51.0.0",
    "ios": {
      "bundleIdentifier": "com.ignaciofabian93.enreglamobile"
    },
    "owner": "dmmtapia"
  }
}
