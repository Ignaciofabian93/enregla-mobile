{
  "expo": {
    "name": "enregla",
    "slug": "enregla-mobile",
    "scheme": "enregla",
    "version": "2.0.2",
    "orientation": "portrait",
    "icon": "./assets/icons/enregla-icon.png",
    "userInterfaceStyle": "light",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "splash": {
      "image": "./assets/icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ignaciofabian93.enreglamobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/enregla-icon.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode": 4,
      "package": "com.ignaciofabian93.enreglamobile",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET"
      ]
    },
    "plugins": [
      "expo-router",
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesClearTextTraffic": true
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permita a enregla utilizar su ubicaci√≥n."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Permita a enregla acceder a sus fotos."
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "runtimeVersion": "2.0.2",
    "sdkVersion": "51.0.0",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0acd45a2-3ca4-44b6-a8fb-b638981b9acb"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/0acd45a2-3ca4-44b6-a8fb-b638981b9acb"
    }
  }
}
